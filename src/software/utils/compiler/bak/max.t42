   What about the stack when looping? */

int max(int i, int j)
{
   if (i < j) {
      i = j;
   }
   return i;
}

void trac42 ()
{
   int x;
   int y;
   int i;

   write "Stop the iteration by entering two equal numbers";
   x = 0;
   y = 100;
   while (!(x == y)) {
      write max(x,y);
      read x;
      read y;
   }
   i = 3;
   if (i == 3)
	i = 4;
}
